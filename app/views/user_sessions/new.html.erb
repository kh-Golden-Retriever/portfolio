<div>
  <%=form_with url: login_path, method: :post, local: true do |f|%>
    <div>
      <%= f.label :email %>
      <%= f.text_field :email %>
    </div>
    <div>
      <%= f.label :password %>
      <%= f.text_field :password %>
    </div>
    <div>
      <%= f.submit %>
    </div>
  <% end %>
</div>
